const { translate: t } = Astro.locals

// const apiUrl_time = "https://timeapi.io/api/Time/current/zone?timeZone=Australia/Adelaide"

---
<LayoutDefault>
  <main un-lg="py-20 min-w-xl animate-fadein-down" class="p-7.5"> 
  
    <div class="flex items-center grid grid-cols-4 gap-4">
    <!-- <div class="flex grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3"> -->

      <div>
        <img class="rounded" style="width:120px" src="/zheyuan_david_avatar_4_5.png" alt="profile picture"> 
      </div>

      <div class="col-start-2 col-span-3">
        <article class="heti">

          <div style="margin-bottom:0.2em;font-size: 11.5pt;"><span class="i-mdi:email-arrow-left">Icon</span>zheyuan.david.liu [at] outlook dot com</div>
          <div style="margin-bottom:0.2em;font-size: 11.5pt;"><span class="i-mdi:contact-phone">Icon</span><a href="https://researchers.adelaide.edu.au/profile/zheyuan.liu">Institute Profile</a></div>
          <div style="margin-bottom:0.2em;font-size: 11.5pt;"><span class="i-mdi:file-download">Icon</span><a href="cv_plain.html">Curriculum Vitae</a></div>
      
        </article>
      </div>

    </div>
  <section contain-layout un-flex="~ col gap-7.5">
    <article class="heti">
      </br>
      </br>
      <About_Content_Main />
    </article>
  </section>  
  
  <section contain-layout un-flex="~ col gap-7.5">
    <article class="heti">
      </br>
      <About_Content />
    </article>
  </section>  

  <section contain-layout un-flex="~ col gap-7.5">
    <article class="heti">
      </br>
      <About_Content_Recent_Research />
    </article>
  </section>  

  <section contain-layout un-flex="~ col gap-7.5">
    <article class="heti">
      </br>
      </br>
      <center><About_Content_News /></center>
    </article>
  </section>  

  <section contain-layout un-flex="~ col gap-7.5">
    <article class="heti">
      <div style="font-size: 13.5pt; font-weight: bold; padding-bottom: 15px"><center>Recent Research</center></div>
      
      <div class="max-w-md mx-auto overflow-hidden md:max-w-2xl">
        <div class="md:flex">
          <div class="p-3 -mb-6 md:shrink-0">
            <Image class="mt-3.5 rounded-md shadow-sm w-48 mx-auto object-cover hover:shadow-lg" style="transition:all .3s ease-in-out;" src={frame_wise_conditioning_img} alt="frame_wise_conditioning_img"> 
          </div>
          <div class="p-3 leading-tight text-slate-700">
            <Research_frame_wise_conditioning />
