          </div>
          <div class="p-3 leading-tight text-slate-700">
            <Research_iccv2023_0_colab />
          </div>
        </div>
      </div>

      <div class="max-w-md mx-auto overflow-hidden md:max-w-2xl">
        <div class="md:flex">
          <div class="p-3 -mb-6 md:shrink-0">
            <Image class="mt-3.5 rounded-md shadow-sm w-48 mx-auto object-cover hover:shadow-lg" style="transition:all .3s ease-in-out;" src={cvpr2023_0_colab_img} alt="cvpr2023_0_colab_img"> 
